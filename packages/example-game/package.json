{
  "name": "@sectjs/example-game",
  "version": "0.1.3",
  "description": "An example game using various packages in the monorepo",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "npm run create-dist-dir && cp src/index.html dist",
    "build": "rollup -c",
    "create-dist-dir": "mkdir -p dist && rm -rf dist/*",
    "lint": "tslint -c ../../tslint.json 'src/**/*.ts'",
    "prepublishOnly": "npm run build",
    "test": "npm run lint",
    "watch": "npm run prebuild && rollup -c -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamesseanwright/tecs.git"
  },
  "keywords": [
    "ecs",
    "entity-component-system",
    "entity",
    "component",
    "system",
    "games",
    "typescript"
  ],
  "author": "James Wright <james@jamesswright.co.uk>",
  "license": "ISC",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/jamesseanwright/tecs/issues"
  },
  "homepage": "https://github.com/jamesseanwright/tecs#readme",
  "dependencies": {
    "@sectjs/basics": "^0.1.3",
    "@sectjs/collision": "^0.1.3",
    "@sectjs/core": "^0.1.3",
    "@sectjs/input": "^0.1.3"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "rollup": "0.58.1",
    "rollup-plugin-node-resolve": "3.3.0",
    "rollup-plugin-typescript2": "0.13.0",
    "rollup-plugin-uglify": "4.0.0",
    "sinon": "4.4.9",
    "uglify-es": "git://github.com/mishoo/UglifyJS2.git#569757d14dbbe04f66af65d7bf0600b3a75efd96"
  },
  "peerDependencies": {
    "typescript": ">=2.8.3"
  }
}
